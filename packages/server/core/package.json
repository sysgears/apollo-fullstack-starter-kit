  "scripts": {
    "tests": "cross-env NODE_ENV=test PORT=7070 wiq test --include babel-polyfill --webpack-config webpack.config.js --full-trace -t 5000 --exit \"src/**/*.spec.js\"",
    "tests:watch": "cross-env NODE_ENV=test PORT=7070 wiq test --include babel-polyfill --webpack-config webpack.config.js --full-trace -t 5000 --watch \"src/**/*.spec.js\"",
