{
  "name": "apollo-fullstack-starter-kit",
  "private": true,
  "version": "1.0.0",
  "main": "src/server",
  "engines": {
    "node": "6.6.0",
    "npm": "3.10.3"
  },
  "scripts": {
    "build": "node tools/webpack.entry",
    "start": "node --harmony build/server",
    "tests": "cross-env NODE_ENV=test PORT=7070 mocha-webpack --include babel-polyfill --webpack-config tools/webpack.entry.js --full-trace --exit \"src/**/*.spec.js\"",
    "tests:watch": "cross-env NODE_ENV=test PORT=7070 mocha-webpack --include babel-polyfill --webpack-config tools/webpack.entry.js --full-trace --watch \"src/**/*.spec.js\"",
    "test": "npm run tests && npm run lint",
    "lint": "eslint --fix --ext js --ext jsx src tests tools",
    "seed": "npm run migrate && knex seed:run",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "watch": "node tools/webpack.entry watch",
    "heroku-postbuild": "rm -rf build && npm run build && npm run seed",
    "cli": "node tools/cli"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sysgears/apollo-fullstack-starter-kit.git"
  },
