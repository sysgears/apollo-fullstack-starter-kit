    "webpack-virtual-modules": "^0.1.5",
    "ws": "^3.0.0",
    "xdl": "^41.0.0"
  },
  "greenkeeper": {
    "ignore": [
      "connect",
      "expo",
      "react",
      "react-dom",
      "react-native",
      "react-test-renderer",
      "subscriptions-transport-ws"
    ]
  }
}
